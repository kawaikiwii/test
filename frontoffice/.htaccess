Options +FollowSymLinks
RewriteEngine on
FileETag none

<FilesMatch "\.(ico|pdf|flv|jpg|jpeg|png|gif|js|css|swf)$">
Header set Cache-Control "public"
Header set Expires "Thu, 15 Apr 2012 20:00:00 GMT"
</FilesMatch>


RewriteBase /
RewriteRule ^(en|fr|fra)$ /index.php?code=$1 [L]
RewriteRule ^(en|fr|fra)/$ /index.php?code=$1 [L]
RewriteRule ^(en|fr|fra)/(.+).php$ /sites/$1/$2.php?code=$1 [L]
RewriteRule .* - [L]